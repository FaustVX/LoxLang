fun fib(n)
{
  fun fib(a, b, n)
  {
    if (n <= 0)
      return a;
    return fib(b, a + b, n - 1);
  }
  return fib(0, 1, n);
}

fun fibSlow(n) {
  if (n <= 1) return n;
  return fibSlow(n - 2) + fibSlow(n - 1);
}

for (var a = 0; a < 15; a = a + 1)
{
  print fibSlow(a);
}
